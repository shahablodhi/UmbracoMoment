            <div class="col-lg-6">
                <div id="grid"></div>
            </div>
            <div class="col-lg-5 pt-10">

                @{
                    count = 0;
                }
                @foreach (var actionBox in page.actionBoxes)
                {
                    <div class="pb-30 pt-30 row">
                        <img class="col-lg-2" src="@Umbraco.Media(actionBox.image.ToString()).umbracoFile" />
                        <a class="col-lg-10 pt-10" style="display: flex;" href="@actionBox.redirectUrl">
                            <span class="custom-heading">@actionBox.title</span>
                            <i style="margin-left: auto" class="fa fa-chevron-right"></i>
                        </a>
                    </div>

                    count++;
                    if (count < page.actionBoxes.Count)
