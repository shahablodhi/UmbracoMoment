<section id="animated-slider" class="carousel slide" data-ride="carousel">
    <div class="container multiselect-container col-xs-9">
        <div class="row">
            <div class="col-lg-2"> &nbsp; </div>
            <div class="col-lg-7">
                <select id="multiselect"></select>
            </div>
            <div class="col-lg-1">
                <a id="findJob" class="button button-border-white custom-button-important ">SOG</a>
            </div>
            <div class="col-lg-2"> &nbsp; </div>
        </div>
    </div>
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#animated-slider" data-slide-to="0" class="active"></li>
        <li data-target="#animated-slider" data-slide-to="1"></li>
    </ol>
    <!-- Carousel inner -->
    <div class="carousel-inner">

        @foreach (var imageUrl in page.backgroundImages)
            {

            <div class="item @(count == 0 ? "active" : "")">
                <img class="img-responsive" src="@Umbraco.Media(imageUrl.ToString()).umbracoFile" alt="slider">
            </div>
            count++;
        }

        <!--/ Carousel item end -->
    </div>


</section>


